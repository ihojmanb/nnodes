<% if @player.errors.any? %>
  <div id="error_explanation">
    <div class= "alert alert-danger">
        El formulario contiene <%= pluralize(@player.errors.count, "error") %>
    </div>
    <ul>
        <% @player.errors.full_messages.each do |msg|%>
            <li><%= msg %></li>
        <% end %>
    </ul>
  </div>
<% end %>
